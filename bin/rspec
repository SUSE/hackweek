#!/usr/bin/env ruby
begin
  load File.expand_path('../spring', __FILE__)
rescue LoadError => e
  raise unless e.message.include?('spring')
end
begin
  load File.expand_path('../spring', __FILE__)
rescue LoadError
  puts 'LoadError: spring is not available'
end

require 'bundler/setup'
load Gem.bin_path('rspec-core', 'rspec')
